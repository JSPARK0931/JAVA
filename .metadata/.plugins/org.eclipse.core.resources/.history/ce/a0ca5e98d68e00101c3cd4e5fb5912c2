package study02;

public class CircleTest {

	public static void main(String[] args) {
		Circle obj = new Circle();
		
		obj.radius = 100; //객체의 필드 접근
		obj.color = "blue"; 
		obj.radius = 1000;
		
		System.out.println(obj.radius);
		System.out.println(obj.color);
		
		System.out.println(obj);
	}

}
